{
  "name": "anthogirard/delivengo-sdk",
  "type": "library",
  "description": "PHP client library for the Delivengo Easy v2.3 API",
  "homepage": "https://www.assistance-mydelivengo.fr/api-easy/",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "Anthony Girard"
    }
  ],
  "autoload": {
    "psr-4": {
      "DelivengoSDK\\": "lib/"
    }
